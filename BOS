local HoverTip = Instance.new("TextLabel")
HoverTip.Name = "HoverTip"
HoverTip.Parent = game:GetService("CoreGui")
HoverTip.Size = UDim2.new(0, 200, 0, 30)
HoverTip.BackgroundColor3 = Color3.fromRGB(140, 140, 140)
HoverTip.BackgroundTransparency = 0.2
HoverTip.TextColor3 = Color3.fromRGB(0, 0, 0)
HoverTip.Text = ""
HoverTip.Font = Enum.Font.SourceSans
HoverTip.TextSize = 14
HoverTip.BorderSizePixel = 0
HoverTip.Visible = false
HoverTip.ZIndex = 10

local function updateHoverTip(text)
    HoverTip.Text = text
    HoverTip.Visible = true
    HoverTip.Position = UDim2.new(0, mouse.X + 10, 0, mouse.Y + 10)
end

local function hideHoverTip()
    HoverTip.Visible = false
end

local function findplr(Target)
    if game:GetService'Players':FindFirstChild(Target) then
        return game:GetService'Players':FindFirstChild(Target)
    else
        for _, player in ipairs(game:GetService'Players':GetPlayers()) do
            if player.Name:lower():find(Target:lower()) or tostring(player.UserId) == Target then
                return player
            end
        end
    end
    return nil
end

local function toggleSetting(settingName, toggleButton)
    Settings[settingName] = not Settings[settingName]
    toggleButton.Text = toggleButton.Name:gsub("Toggle$", ""):gsub("([A-Z])", " %1"):gsub("^ ", "") .. ": " .. tostring(Settings[settingName])

    if settingName == "Autofarm" then
        if Settings.Autofarm then
            spawn(function()
                while Settings.Autofarm do
                    wait(0.1)
                end
            end)
        end
    elseif settingName == "AutoPunch" then
        if Settings.AutoPunch then
            spawn(function()
                while Settings.AutoPunch do
                    wait(0.05)
                    vim:SendKeyEvent(true, Enum.KeyCode.E, false, game)
                    vim:SendKeyEvent(false, Enum.KeyCode.E, false, game)
                end
            end)
        end
    elseif settingName == "SuperPunch" then
        if Settings.SuperPunch then

        else

        end
    elseif settingName == "Virus" then
        if Settings.Virus then

            spawn(function()
                while Settings.Virus do
                    wait(1)
                    for _, player in ipairs(plrs:GetPlayers()) do
                        if player ~= plr and player.Character and player.Character:FindFirstChild("Humanoid") then

                        end
                    end
                end
            end)
        else

        end
    elseif settingName == "AntiAnvil" then
        if Settings.AntiAnvil then

        else

        end
    elseif settingName == "FakeAccs" then
        if Settings.FakeAccs then

        else

        end
    elseif settingName == "AntiSpinner" then
        if Settings.AntiSpinner then

            pcall(function()
                repeat wait() until plr.Character and plr.Character:FindFirstChild'HumanoidRootPart' and plr.Character:FindFirstChild'Head'
                for _,v in next,plr.Character:GetChildren() do
                    if v:IsA'BasePart' then
                        v.CustomPhysicalProperties = prop1000
                    end
                end
            end)
        else

        end
    elseif settingName == "AntiPunch" then
        if Settings.AntiPunch then

        else

        end
    elseif settingName == "AntiFling" then
        if Settings.AntiFling then

            spawn(function()
                local char = plr.Character
                if not char then return end
                local hrp = char:FindFirstChild("HumanoidRootPart")
                if not hrp then return end

                LoopFlingV1[plr] = rs.Heartbeat:Connect(function()
                    if hrp.Velocity.Magnitude > 50 then
                        hrp.Velocity = hrp.Velocity * 0.5
                    end
                end)
                LoopFlingV2[plr] = char.Humanoid.FreeFalling:Connect(function(isFalling)
                    if isFalling and hrp.Velocity.Magnitude > 50 then
                        hrp.CFrame = hrp.CFrame - hrp.Velocity * 0.1
                        hrp.Velocity = vec0
                        hrp.RotVelocity = vec0
                    end
                end)
            end)
        else

            if LoopFlingV1[plr] then LoopFlingV1[plr]:Disconnect() LoopFlingV1[plr] = nil end
            if LoopFlingV2[plr] then LoopFlingV2[plr]:Disconnect() LoopFlingV2[plr] = nil end
        end
    elseif settingName == "AntiVelocity" then
        if Settings.AntiVelocity then

            spawn(function()
                while Settings.AntiVelocity and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") do
                    plr.Character.HumanoidRootPart.Velocity = vec0
                    plr.Character.HumanoidRootPart.RotVelocity = vec0
                    wait()
                end
            end)
        else

        end
    elseif settingName == "LessPing" then
        if Settings.LessPing then

            spawn(function()
                pcall(function()
                    repeat rs.RenderStepped:wait() until plr.Character and plr.Character:FindFirstChild'BodyFollowMouse' and plr.Character:FindFirstChild'HeadWaist'
                    if plr.Character and plr.Character:FindFirstChild'BodyFollowMouse' then
                        plr.Character.BodyFollowMouse.Disabled = true
                    end
                    if plr.Character and plr.Character:FindFirstChild'HeadWaist' then
                        plr.Character.HeadWaist:FireServer(CFrame.new(0,0.8,0),CFrame.new(0,0.2,0))
                    end
                end)
            end)
        else

            pcall(function()
                if plr.Character and plr.Character:FindFirstChild'BodyFollowMouse' then
                    plr.Character.BodyFollowMouse.Disabled = false
                end
            end)
        end
    elseif settingName == "Toggle1" then

    elseif settingName == "Toggle2" then

    elseif settingName == "Toggle3" then

    end
end

