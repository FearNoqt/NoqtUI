local function createToggle(parentFrame, text, settingName)
    local toggle = Instance.new("TextButton")
    toggle.Name = text:gsub(" ", "") .. "Toggle"
    toggle.Parent = parentFrame
    toggle.Size = UDim2.new(0, 150, 0, 22)
    toggle.Position = UDim2.new(0, parentFrame.posX, 0, parentFrame.posY)
    toggle.BackgroundColor3 = Color3.fromRGB(163, 163, 163)
    toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
    toggle.Text = text .. ": " .. tostring(Settings[settingName])
    toggle.Font = Enum.Font.SourceSans
    toggle.TextSize = 14
    toggle.BorderSizePixel = 0

    parentFrame.posX = parentFrame.posX + 150 + 6
    if parentFrame.posX > 360 then
        parentFrame.posX = 6
        parentFrame.posY = parentFrame.posY + 28
    end

    toggle.MouseButton1Click:Connect(function()
        toggleSetting(settingName, toggle)
    end)

    toggle.MouseEnter:Connect(function()
        updateHoverTip("Toggles the " .. text .. " feature.")
    end)
    toggle.MouseLeave:Connect(hideHoverTip)

    return toggle
end

