local function applyTheme(theme)
    Frame.BackgroundColor3 = theme.FrameBackground
    Frame.BackgroundTransparency = theme.FrameTransparency
    Open.BackgroundColor3 = theme.OpenButtonBackground
    Open.TextColor3 = theme.OpenButtonText
    Refresh.BackgroundColor3 = theme.RefreshButtonBackground
    Refresh.TextColor3 = theme.RefreshButtonText
    Respawn.BackgroundColor3 = theme.RespawnButtonBackground
    Respawn.TextColor3 = theme.RespawnButtonText
    HoverTip.BackgroundColor3 = theme.HoverTipBackground
    credits.TextColor3 = theme.CreditsText
    version.TextColor3 = theme.VersionText
    ping.BackgroundColor3 = theme.PingTextBackground
    points.BackgroundColor3 = theme.PointsTextBackground
    ping.TextColor3 = theme.PingPointsText
    points.TextColor3 = theme.PingPointsText

    for _, pageButton in next, PageHolder:GetChildren() do
        if pageButton:IsA("TextButton") and pageButton.Name ~= "Open" and pageButton.Name ~= "Refresh" and pageButton.Name ~= "Respawn" then
            pageButton.BackgroundColor3 = theme.PageButtonBackground
            pageButton.TextColor3 = theme.PageButtonText
        end
    end

    for _, pageFrame in next, Holder:GetChildren() do
        if pageFrame:IsA("Frame") then
            for _, child in next, pageFrame:GetChildren() do
                if child:IsA("TextButton") and child.Name:find("Toggle") then
                    child.BackgroundColor3 = theme.PageButtonBackground
                    child.TextColor3 = theme.PageButtonText
                elseif child:IsA("TextBox") then
                    child.BackgroundColor3 = theme.BoxBackground
                    child.TextColor3 = theme.BoxText
                end
            end
        end
    end
end

local ThemeDropdown = Instance.new("ComboBox", Misc)
ThemeDropdown.Name = "ThemeSelector"
ThemeDropdown.Size = UDim2.new(0, 150, 0, 22)
ThemeDropdown.Position = UDim2.new(0, table.Misc.posX, 0, table.Misc.posY)
ThemeDropdown.PlaceholderText = "Select Theme"
ThemeDropdown.Font = Enum.Font.SourceSans
ThemeDropdown.TextColor3 = Color3.fromRGB(0, 0, 0)
ThemeDropdown.BackgroundColor3 = Color3.fromRGB(163, 163, 163)
ThemeDropdown.BackgroundTransparency = 0.500

for _, theme in ipairs(Themes) do
    ThemeDropdown:AddItem(theme.Name)
end

ThemeDropdown.ItemSelected:Connect(function(item)
    for _, theme in ipairs(Themes) do
        if theme.Name == item then
            applyTheme(theme)
            break
        end
    end
end)

table.Misc.posX = table.Misc.posX + 150 + 6
if table.Misc.posX > 360 then
    table.Misc.posX = 6
    table.Misc.posY = table.Misc.posY + 28
end

applyTheme(Themes[1])

local isGuiOpen = true

Open.MouseButton1Click:Connect(function()
    isGuiOpen = not isGuiOpen
    Frame.Visible = isGuiOpen
    Open.Text = isGuiOpen and "Close" or "Open"
end)

Frame.Visible = isGuiOpen
Open.Text = isGuiOpen and "Close" or "Open"

game:GetService'RunService'.Heartbeat:Connect(function()
    if Settings['AntiAnvil'] then
        for _,v in next,plrs:GetPlayers() do
            pcall(function()
                if v.Character:FindFirstChild'Anvil' then
                    v.Character.Anvil.send:FireServer(v.Character.HumanoidRootPart.CFrame.p+Vector3.new(0,10,0))
                end
            end)
        end
    end
end)

plr.CharacterAdded:Connect(function()
    pcall(function()
        if Settings['AntiSpinner'] then
            repeat wait() until plr.Character:FindFirstChild'HumanoidRootPart' and plr.Character:FindFirstChild'Head'
            for _,v in next,plr.Character:GetChildren() do
                if v:IsA'BasePart' then
                    v.CustomPhysicalProperties = prop1000
                end
            end
        end
        if Settings['LessPing'] then
            spawn(function()
                pcall(function()
                    repeat rs.RenderStepped:wait() until plr.Character:FindFirstChild'BodyFollowMouse' and plr.Character:FindFirstChild'HeadWaist'
                    if plr.Character and plr.Character:FindFirstChild'BodyFollowMouse' then
                        plr.Character.BodyFollowMouse.Disabled = true
                    end
                    if plr.Character and plr.Character:FindFirstChild'HeadWaist' then
                        plr.Character.HeadWaist:FireServer(CFrame.new(0,0.8,0),CFrame.new(0,0.2,0))
                    end
                end)
            end)
        end
    end)
end)

spawn(function()
    while true do
        wait(1)
        pinga = game:GetService'Stats'.Network.Ping:GetValue()
        if plr and plr.leaderstats and plr.leaderstats:FindFirstChild("Points") then
            points = plr.leaderstats.Points.Value
        else
            points = 0
        end
        ping.Text = "Ping: " .. math.floor(pinga * 1000) .. "ms"
        points.Text = "Points: " .. points
    end
end)
