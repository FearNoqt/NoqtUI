local table = {
    Global = {page = Instance.new("Frame", Holder), button = Instance.new("TextButton", PageHolder), posX = 6, posY = 6},
    Combat = {page = Instance.new("Frame", Holder), button = Instance.new("TextButton", PageHolder), posX = 6, posY = 6},
    Visual = {page = Instance.new("Frame", Holder), button = Instance.new("TextButton", PageHolder), posX = 6, posY = 6},
    Movement = {page = Instance.new("Frame", Holder), button = Instance.new("TextButton", PageHolder), posX = 6, posY = 6},
    Misc = {page = Instance.new("Frame", Holder), button = Instance.new("TextButton", PageHolder), posX = 6, posY = 6},
}

for i,v in next, table do
    v.page.Name = i
    v.page.Size = UDim2.new(1, 0, 1, 0)
    v.page.Position = UDim2.new(0, 0, 0, 0)
    v.page.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    v.page.BackgroundTransparency = 1
    v.page.BorderSizePixel = 0
    v.page.Visible = false

    v.button.Name = i
    v.button.Size = UDim2.new(1, 0, 0, 28)
    v.button.BackgroundColor3 = Color3.fromRGB(163, 163, 163)
    v.button.TextColor3 = Color3.fromRGB(0, 0, 0)
    v.button.Text = i
    v.button.Font = Enum.Font.SourceSans
    v.TextSize = 18
    v.BorderSizePixel = 0

    if i == "Global" then
        v.page.Visible = true
    end

    local layout = Instance.new("UIListLayout", PageHolder)
    layout.FillDirection = Enum.FillDirection.Vertical
    layout.Padding = UDim.new(0, 2)
    layout.SortOrder = Enum.SortOrder.LayoutOrder

    v.button.LayoutOrder = layout.LayoutOrder + 1

    v.button.MouseButton1Click:Connect(function()
        for _,x in next, table do
            x.page.Visible = false
        end
        v.page.Visible = true
    end)
end

