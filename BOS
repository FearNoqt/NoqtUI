local plr = plrs.LocalPlayer
local vec0 = Vector3.new()
local prop1000 = PhysicalProperties.new(1000,1000,0,1000,0)
local cf0 = CFrame.new()

local Settings = {
    ['Autofarm'] = false,
    ['AutoPunch'] = false,
    ['SuperPunch'] = false,
    ['Virus'] = false,
    ['AntiAnvil'] = false,
    ['FakeAccs'] = false,
    ['AntiSpinner'] = false,
    ['AntiPunch'] = false,
    ['AntiFling'] = false,
    ['AntiVelocity'] = false,
    ['LessPing'] = false,
